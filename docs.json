{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Solo Docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "guide/intro",
              "guide/solo-server"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "guide/custom-modules",
              "guide/deploying-on-k8s"
            ]
          },
          {
            "group": "Additional Info",
            "pages": [
              "guide/telemetry",
              "guide/best-practices",
              "guide/migration"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Solo Server API",
            "pages": [
              "api/solo-server/introduction",
              "api/solo-server/endpoints"
            ]
          },
          {
            "group": "FastMCP API",
            "pages": [
              "api/fastmcp/introduction"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/GetSoloTech",
          "icon": "github"
        },
        {
          "anchor": "Solo Website",
          "href": "https://solotech.ai",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Solo Server",
        "href": "https://github.com/GetSoloTech/solo-server"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Solovision",
      "href": "https://github.com/GetSoloTech/solovision"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/GetSoloTech",
      "github": "https://github.com/GetSoloTech",
      "linkedin": "https://www.linkedin.com/company/get-solo-tech"
    }
  }
}